<?xml version="1.0" encoding="ASCII"?>
<acmm:AbstractComponentSpecification
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:acmm="http://acg.ual.es/definitions/abstractComponent/1.3.2"
    xsi:schemaLocation="http://acg.ual.es/definitions/abstractComponent/1.3.2 ../model/acmm.ecore"
    componentID="twitter-less"
    componentDescription="Twitter"
    componentName="twitter">
  <functional>
    <providedInterfaces>
      <interface
          interfaceID="manageTwitter">
        <interfaceSpecification
            uri="http://acg.ual.es/ac/twitter/manageTwitter.wsdl">
          <portType
              name="manageTwitter">
            <operation
                name="filter">
              <input
                  name="filter"
                  type="trader:filterInput">
                <element
                    name="filterElement"
                    type="trader:filterText"/>
              </input>
            </operation>
            <operation
                name="removeAvatar">
              <input
                  name="removeAvatar"
                  type="trader:removeAvatarInput"/>
            </operation>
          </portType>
        </interfaceSpecification>
      </interface>
    </providedInterfaces>
  </functional>
</acmm:AbstractComponentSpecification>
