<?xml version="1.0" encoding="ASCII"?>
<acmm:AbstractComponentSpecification
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:acmm="http://acg.ual.es/definitions/abstractComponent/1.3.2"
    xsi:schemaLocation="http://acg.ual.es/definitions/abstractComponent/1.3.2 ../model/acmm.ecore"
    componentID="cloudStorage"
    componentDescription="CloudStorage"
    componentName="cloudStorage">
  <functional>
    <providedInterfaces>
      <interface
          interfaceID="manageImage">
        <interfaceSpecification
            uri="http://acg.ual.es/ac/cloudStorage/manageImage.wsdl">
          <portType
              name="manageImage">
            <operation
                name="getImage">
              <input
                  name="getImage"
                  type="trader:getImageInput">
                <element
                    name="imageURI"
                    type="trader:imageURI"
                    minOccurs="1"
                    maxOccurs="1"/>
              </input>
              <output
                  name="getImageResponse"
                  type="trader:getImageOutput">
                <element
                    name="image"
                    type="trader:image"
                    minOccurs="1"
                    maxOccurs="1"/>
              </output>
            </operation>
          </portType>
        </interfaceSpecification>
      </interface>
    </providedInterfaces>
  </functional>
</acmm:AbstractComponentSpecification>
