<?xml version="1.0" encoding="ASCII"?>
<acmm:AbstractComponentSpecification
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:acmm="http://acg.ual.es/definitions/abstractComponent/1.3.2"
    xsi:schemaLocation="http://acg.ual.es/definitions/abstractComponent/1.3.2 ../model/acmm.ecore"
    componentID="twitter-mark"
    componentDescription="Twitter"
    componentName="twitter-mark">
  <functional>
    <providedInterfaces>
      <interface
          interfaceID="manageTwitter">
        <interfaceSpecification
            uri="http://acg.ual.es/ac/twitter/manageTwitter.wsdl">
          <portType
              name="manageTwitter">
            <operation
                name="filter">
              <input
                  name="filter"
                  type="trader:filterInput">
                <element
                    name="filterElement"
                    type="trader:filterText"/>
              </input>
            </operation>
            <operation
                name="removeAvatar">
              <input
                  name="removeAvatar"
                  type="trader:remoteAvatarOutput"/>
            </operation>
          </portType>
        </interfaceSpecification>
      </interface>
    </providedInterfaces>
    <requiredInterfaces>
      <interface
          interfaceID="manageImage">
        <interfaceSpecification
            uri="http://acg.ual.es/ac/twitter/manageImage.wsdl">
          <portType
              name="manageImage">
            <operation
                name="getImage">
              <input
                  name="getImage"
                  type="trader:getImageInput">
                <element
                    name="imageURI"
                    type="trader:imageURI"
                    minOccurs="1"
                    maxOccurs="1"/>
              </input>
              <output
                  name="getImageResponse"
                  type="trader:getImageOutput">
                <element
                    name="image"
                    type="trader:image"
                    minOccurs="1"
                    maxOccurs="1"/>
              </output>
            </operation>
          </portType>
        </interfaceSpecification>
      </interface>
      <interface
          interfaceID="manageUser">
        <interfaceSpecification
            uri="http://acg.ual.es/ac/twitter/manageUser.wsdl">
          <portType
              name="manageUser">
            <operation
                name="getUserInfo">
              <input
                  name="getUserInfo"
                  type="trader:getUserInfoInput"/>
              <output
                  name="getUserInfoResponse"
                  type="trader:getUserInfoOutput">
                <element
                    name="userInfo"
                    type="trader:userInfo"
                    minOccurs="1"
                    maxOccurs="1"/>
              </output>
            </operation>
          </portType>
        </interfaceSpecification>
      </interface>
    </requiredInterfaces>
  </functional>
  <extraFunctional>
    <dependencies>
      <dependency
          interfaceID="manageUser"/>
    </dependencies>
  </extraFunctional>
  <marketing
      entityID="ACG">
    <contacts>
      <contact
          personName="Javi"/>
    </contacts>
  </marketing>
</acmm:AbstractComponentSpecification>
