    componentID="http://acg.ual.es/wookie/widgets/rss08"
    abstractComponentID="rss"
    componentDescription="RSS"
    componentName="rss08">
  <functional>
    <interactionContent
        filesLanguage="Javascript"
        filesAddress="./content/interaction/"/>
    <coreContent
        filesLanguage="Javascript"
        filesAddress="./content/core/"/>
    <controllerInterface
        filesLanguage="Javascript"
        filesAddress="./interface/controller/">
      <providedInterfaces>
        <interface
            interfaceID="manageRSS">
          <interfaceSpecification
              uri="http://acg.ual.es/cc/rss3/manageRSS.wsdl">
            <portType
                name="manageRSS">
              <operation
                  name="changeWidth">
                <input
                    name="changeWidth"
                    type="trader:changeWidthInput">
                  <element
                      name="width"
                      type="trader:width"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
              <operation
                  name="changeHeight">
                <input
                    name="changeHeight"
                    type="trader:changeHeightInput">
                  <element
                      name="height"
                      type="trader:height"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
              <operation
                  name="changeRSSFeed">
                <input
                    name="changeRSSFeed"
                    type="trader:changeRSSFeedInput">
                  <element
                      name="rssFeed"
                      type="trader:rssFeed"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
              <operation
                  name="setRSSLength">
                <input
                    name="setRSSLength"
                    type="trader:setRSSLengthInput">
                  <element
                      name="rssLength"
                      type="trader:rssLength"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
              <operation
                  name="getRSSLength">
                <input
                    name="getRSSLength"
                    type="trader:getRSSLengthInput"/>
                <output
                    name="getRSSLengthResponse"
                    type="trader:getRSSLengthOutput">
                  <element
                      name="rssLength"
                      type="trader:rssLegnth"
                      minOccurs="1"
                      maxOccurs="1"/>
                </output>
              </operation>
              <operation
                  name="setRSSFormat">
                <input
                    name="setRSSFormat"
                    type="trader:setRSSFormatInput">
                  <element
                      name="rssFormat"
                      type="trader:rssFormat"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
              <operation
                  name="getRSSFormat">
                <input
                    name="getRSSFormat"
                    type="trader:getRSSFormatInput"/>
                <output
                    name="getRSSFormatResponse"
                    type="trader:getRSSFormatOutput">
                  <element
                      name="rssFormat"
                      type="trader:rssFormat"
                      minOccurs="1"
                      maxOccurs="1"/>
                </output>
              </operation>
            </portType>
          </interfaceSpecification>
        </interface>
      </providedInterfaces>
    </controllerInterface>
    <interactionInterface
        filesLanguage="Javascript"
        filesAddress="./interface/interaction"/>
