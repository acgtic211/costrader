                  <element
                      name="color"
                      type="trader:color"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
              <operation
                  name="changeFontSize">
                <input
                    name="changeFontSize"
                    type="trader:changeFontSizeInput">
                  <element
                      name="fontSize"
                      type="trader:fontSize"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
            </portType>
          </interfaceSpecification>
        </interface>
      </providedInterfaces>
    </controllerInterface>
    <interactionInterface
        filesLanguage="Javascript"
        filesAddress="./interface/interaction"/>
