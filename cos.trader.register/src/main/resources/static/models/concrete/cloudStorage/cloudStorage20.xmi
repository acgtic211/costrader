<?xml version="1.0" encoding="ASCII"?>
<ccmm:ConcreteComponentSpecification
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ccmm="http://acg.ual.es/definitions/concreteComponent/1.3.2"
    xsi:schemaLocation="http://acg.ual.es/definitions/concreteComponent/1.3.2 ../../model/ccmm.ecore"
    componentID="http://acg.ual.es/wookie/widgets/cloudStorage20"
    abstractComponentID="cloudStorage"
    componentDescription="Cloud Storage"
    componentName="cloudStorage20">
  <functional>
    <interactionContent
        filesLanguage="Javascript"
        filesAddress="./content/interaction/"/>
    <coreContent
        filesLanguage="Javascript"
        filesAddress="./content/core/"/>
    <controllerInterface
        filesLanguage="Javascript"
        filesAddress="./interface/controller/">
      <providedInterfaces>
        <interface
            interfaceID="getImage">
          <interfaceSpecification
              uri="http://acg.ual.es/cc/cloudStorage4/getImage.wsdl">
            <portType
                name="getImage">
              <operation
                  name="getImage">
                <input
                    name="getImage"
                    type="trader:getImageInput">
                  <element
                      name="imageURI"
                      type="trader:imageURI"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
                <output
                    name="getImageResponse"
                    type="trader:getImageInput">
                  <element
                      name="imageData"
                      type="trader:imageData"
                      minOccurs="1"
                      maxOccurs="1"/>
                </output>
              </operation>
            </portType>
          </interfaceSpecification>
        </interface>
        <interface
            interfaceID="getFile">
          <interfaceSpecification
              uri="http://acg.ual.es/cc/cloudStorage4/getFile.wsdl">
            <portType
                name="getFile">
              <operation
                  name="getFile">
                <input
                    name="getFile"
                    type="trader:getFileInput">
                  <element
                      name="fileType"
                      type="trader:fileType"
                      maxOccurs="1"/>
                  <element
                      name="fileURI"
                      type="trader:fileURI"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
            </portType>
          </interfaceSpecification>
        </interface>
        <interface
            interfaceID="manageSession">
          <interfaceSpecification
              uri="http://acg.ual.es/cc/cloudStorage4/manageSession.wsdl">
            <portType
                name="manageSession">
              <operation
                  name="doLogin">
                <input
                    name="doLogin"
                    type="trader:doLoginInput">
                  <element
                      name="userID"
                      type="trader:userID"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
              <operation
                  name="doLogout">
                <input
                    name="doLogout"
                    type="trader:doLogoutInput">
                  <element
                      name="userID"
                      type="trader:userID"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
            </portType>
          </interfaceSpecification>
        </interface>
      </providedInterfaces>
      <requiredInterfaces>
        <interface
            interfaceID="manageLocalFiles">
          <interfaceSpecification
              uri="http://acg.ual.es/cc/cloudStorage4/manageLocalFiles.wsdl">
            <portType
                name="manageLocalFiles">
              <operation
                  name="readLocalFiles">
                <input
                    name="readLocalFiles"
                    type="trader:readLocalFilesInput">
                  <element
                      name="localURI"
                      type="trader:localURI"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
                <output
                    name="readLocalFilesResponse"
                    type="trader:readLocalFilesOutput">
                  <element
                      name="localFilesData"
                      type="trader:localFilesData"
                      minOccurs="1"
                      maxOccurs="1"/>
                </output>
              </operation>
            </portType>
          </interfaceSpecification>
        </interface>
      </requiredInterfaces>
    </controllerInterface>
    <interactionInterface
        filesLanguage="Javascript"
        filesAddress="./interface/interaction"/>
  </functional>
  <packaging>
    <location
        repositoryID="WookieACG"
        repositoryType="ApacheWookie_0_15_0"
        repositoryURI="http://acg.ual.es/wookie/"
        componentURI="http://acg.ual.es/wookie/widgets"/>
    <implementation
        programmingLanguage="HTML-CSS-Javascript"/>
    <version
        versionID="1.1.0"
        versionDate="2014-03-20"/>
  </packaging>
  <marketing
      entityID="ACG"
      entityName="Applied Computing Group"
      entityDescription="">
    <contacts>
      <contact
          personName="Jes&#xed;s Vallecillos Ruiz"
          email="jesus.vallecillos@ual.es"
          phone="+34950214618"
          address="Ctra. Sacramento s/n"/>
    </contacts>
  </marketing>
</ccmm:ConcreteComponentSpecification>
