<?xml version="1.0" encoding="ASCII"?>
<ccmm:ConcreteComponentSpecification
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ccmm="http://acg.ual.es/definitions/concreteComponent/1.3.2"
    xsi:schemaLocation="http://acg.ual.es/definitions/concreteComponent/1.3.2 ../../model/ccmm.ecore"
    componentID="http://acg.ual.es/wookie/widgets/cloudStorage15"
    abstractComponentID="cloudStorage"
    componentDescription="Cloud Storage"
    componentName="cloudStorage15">
  <functional>
    <interactionContent
        filesLanguage="Javascript"
        filesAddress="./content/interaction/"/>
    <coreContent
        filesLanguage="Javascript"
        filesAddress="./content/core/"/>
    <controllerInterface
        filesLanguage="Javascript"
        filesAddress="./interface/controller/">
      <providedInterfaces>
        <interface
            interfaceID="manageFile">
          <interfaceSpecification
              uri="http://acg.ual.es/cc/cloudStorage3/manageFile.wsdl">
            <portType
                name="manageFile">
              <operation
                  name="getFile">
                <input
                    name="getFile"
                    type="trader:getFileInput">
                  <element
                      name="fileType"
                      type="trader:fileType"
                      maxOccurs="1"/>
                  <element
                      name="fileURI"
                      type="trader:fileURI"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
              <operation
                  name="addFile">
                <input
                    name="addFile"
                    type="trader:addFileInput">
                  <element
                      name="fileType"
                      type="trader:fileType"
                      minOccurs="1"
                      maxOccurs="1"/>
                  <element
                      name="fileURI"
                      type="trader:fileURI"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
              <operation
                  name="deleteFile">
                <input
                    name="deleteFile"
                    type="trader:deleteFileInput">
                  <element
                      name="fileURI"
                      type="trader:fileURI"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
              <operation
                  name="getImage">
                <input
                    name="getImage"
                    type="trader:getImageInput">
                  <element
                      name="imageURI"
                      type="trader:imageURI"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
                <output
                    name="getImageResponse"
                    type="trader:getImageOutput">
                  <element
                      name="imageData"
                      type="trader:imageData"
                      minOccurs="1"
                      maxOccurs="1"/>
                </output>
              </operation>
            </portType>
          </interfaceSpecification>
        </interface>
      </providedInterfaces>
    </controllerInterface>
    <interactionInterface
        filesLanguage="Javascript"
        filesAddress="./interface/interaction"/>
  </functional>
  <packaging>
    <location
        repositoryID="WookieACG"
        repositoryType="ApacheWookie_0_15_0"
        repositoryURI="http://acg.ual.es/wookie/"
        componentURI="http://acg.ual.es/wookie/widgets"/>
    <implementation
        programmingLanguage="HTML-CSS-Javascript"/>
    <version
        versionID="1.1.0"
        versionDate="2014-03-20"/>
  </packaging>
  <marketing
      entityID="ACG"
      entityName="Applied Computing Group"
      entityDescription="">
    <contacts>
