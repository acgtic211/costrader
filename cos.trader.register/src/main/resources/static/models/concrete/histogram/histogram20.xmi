<?xml version="1.0" encoding="ASCII"?>
<ccmm:ConcreteComponentSpecification
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ccmm="http://acg.ual.es/definitions/concreteComponent/1.3.2"
    xsi:schemaLocation="http://acg.ual.es/definitions/concreteComponent/1.3.2 ../../model/ccmm.ecore"
    componentID="http://acg.ual.es/wookie/widgets/histogram20"
    abstractComponentID="histogram"
    componentDescription="Histogram"
    componentName="histogram20">
  <functional>
    <interactionContent
        filesLanguage="Javascript"
        filesAddress="./content/interaction/"/>
    <coreContent
        filesLanguage="Javascript"
        filesAddress="./content/core/"/>
    <controllerInterface
        filesLanguage="Javascript"
        filesAddress="./interface/controller/">
      <providedInterfaces>
        <interface
            interfaceID="manageHistogram">
          <interfaceSpecification
              uri="http://acg.ual.es/cc/histogram1/manageHistogram.wsdl">
            <portType
                name="manageHistogram">
              <operation
                  name="setHistogramLayers">
                <input
                    name="setHistogramLayers"
                    type="trader:setHistogramLayersInput">
                  <element
                      name="mapLayers"
                      type="trader:mapLayers"/>
                </input>
              </operation>
            </portType>
          </interfaceSpecification>
        </interface>
        <interface
            interfaceID="managePalette"
            interfaceDescription="">
          <interfaceSpecification
              uri="http://acg.ual.es/cc/histogram1/managePalette.wsdl">
            <portType
                name="managePalette">
              <operation
                  name="changePalette">
                <input
                    name="changePalette"
                    type="trader:changePaletteInput">
                  <element
                      name="histogramPalette"
                      type="trader:histogramPalette"
                      minOccurs="1"
                      maxOccurs="1"/>
                </input>
              </operation>
            </portType>
          </interfaceSpecification>
        </interface>
      </providedInterfaces>
      <requiredInterfaces>
        <interface
            interfaceID="manageMap">
          <interfaceSpecification
              uri="http://acg.ual.es/cc/histogram1/manageMap.wsdl">
            <portType
                name="manageMap">
              <operation
                  name="getMapLayers">
                <input
                    name="getMapLayers"
                    type="trader:getMapLayersInput"/>
                <output
                    name="getMapLayersResponse"
                    type="trader:getMapLayersOutput">
                  <element
                      name="mapLayers"
                      type="trader:mapLayers"/>
                </output>
              </operation>
            </portType>
          </interfaceSpecification>
        </interface>
      </requiredInterfaces>
    </controllerInterface>
    <interactionInterface
        filesLanguage="Javascript"
        filesAddress="./interface/interaction"/>
  </functional>
  <packaging>
    <location
        repositoryID="WookieACG"
        repositoryType="ApacheWookie_0_15_0"
        repositoryURI="http://acg.ual.es/wookie/"
        componentURI="http://acg.ual.es/wookie/widgets"/>
    <implementation
        programmingLanguage="HTML-CSS-Javascript"/>
    <version
        versionID="1.1.0"
        versionDate="2014-03-20"/>
  </packaging>
  <marketing
      entityID="ACG"
      entityName="Applied Computing Group"
      entityDescription="">
    <contacts>
      <contact
          personName="Jes&#xed;s Vallecillos Ruiz"
          email="jesus.vallecillos@ual.es"
          phone="+34950214618"
          address="Ctra. Sacramento s/n"/>
    </contacts>
  </marketing>
</ccmm:ConcreteComponentSpecification>
